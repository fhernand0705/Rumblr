<title>Dashboard</title>
<div class="main-container">

<%= erb :nav %>
  <div class="header-container">
    <h2 class="dash-header">Community Posts</h2>
  </div>
  <div class="dash-container">
    <% @posts.each do |p| %>
      <h2 class="post-title"><%= p.title %></h2>
      <textarea class="text-box" name="text" rows="25" cols="30" readonly><%= p.content %></textarea>
    <% end %>
  </div>
</div>

<%= erb :footer %>
